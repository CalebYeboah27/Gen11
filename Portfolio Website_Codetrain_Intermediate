    <script type="text/javascript">
      var ScrollObserver = () => {
        console.log('ready')
        let navBarClassList = document.querySelector(".navbar").classList
        window.addEventListener("scroll",  function(e){
            console.log('add background-color')
          if(window.scrollY >= 52){ 
            navBarClassList.add("navscroll")
          }

          else navBarClassList.remove("navscroll")


        });
        
      }

      ScrollObserver()
    </script>
